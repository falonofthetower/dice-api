{
  "type": "array",
  "additionalItems": false,
  "items": {
    "$id": "#/items",
    "anyOf": [
      {
        "type": "object",
        "required": ["id", "value"],
        "properties": {
          "id": {
            "type": "integer"
          },
          "value": {
            "type": "number",
            "multipleOf": 1,
            "minimum": 1,
            "maximum": 6
          }
        },
        "additionalProperties": false
      }
    ]
  }
}
